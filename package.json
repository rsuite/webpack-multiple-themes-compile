{
  "name": "webpack-multiple-themes-compile",
  "version": "3.0.0",
  "description": "Overwrite webpack config to output multiple themes css.",
  "main": "lib/index.js",
  "scripts": {
    "test": "rm -rf src/src && rm -rf test/themes && jest --silent --coverage --coverageReporters=text-lcov",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "build": "rm -rf lib && rm -rf src/src && babel src --out-dir lib",
    "ready": "npm run test && npm run build",
    "prepublishOnly": "npm run ready",
    "preversion": "npm run ready"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:rsuite/webpack-multiple-themes-compile.git"
  },
  "author": "Godefery.Yang <hiyangguo@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "less": "*",
    "less-loader": "*",
    "mini-css-extract-plugin": "*",
    "css-loader": "*"
  },
  "dependencies": {
    "fs-extra": "^8.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "\\.js$": "babel-jest"
    }
  },
  "files": [
    "README.md",
    "lib",
    "src"
  ],
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/preset-env": "^7.5.5",
    "babel-jest": "^24.9.0",
    "coveralls": "^3.0.2",
    "css-loader": "^6.3.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "less": "^4.1.1",
    "less-loader": "^10.0.1",
    "mini-css-extract-plugin": "^2.3.0",
    "regenerator-runtime": "^0.12.1",
    "webpack": "^5.54.0",
    "webpack-merge": "^4.1.4"
  },
  "keywords": [
    "webpack",
    "less-loader",
    "multiple-css",
    "multiple-less",
    "multiple-themes"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
